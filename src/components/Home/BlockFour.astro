---
import Headline from "../shared/Headline.astro";
import Wrapper from "../shared/Wrapper.astro";
import "./blockFour.scss";
---

<div class="homeb4">
  <div class="homeb4-header">
    <div class="homeb4-bg"></div>
    <Wrapper classes="homeb4-wrapper">
      <div class="homeb4-title">
        <Headline ele="h2" hlstyle="two">{Astro.props.data.title}</Headline>
        <div set:html={Astro.props.data.content} />
      </div>
    </Wrapper>
  </div>
  <div class="homeb4-video">
    <div set:html={Astro.props.data.video} />
  </div>
</div>
